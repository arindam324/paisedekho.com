generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id @unique @default(uuid())
  name     String
  email    String @unique
  password String
  role     Role   @default(User)
}

enum Role {
  Admin
  User
}

model Products {
  id           String @id @unique @default(uuid())
  title        String
  description  String
  redirectText String
}
